{
    "manifest_version": 3,
    "name": "Word Replacer",
    "version": "1.0",
    "description": "Replace specific words on webpages",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
}
  